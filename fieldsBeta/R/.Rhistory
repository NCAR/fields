help( spatialProcess)
library( fields)
help( spatialProcess)
data( ozone2)
# x is a two column matrix where each row is a location in lon/lat
# coordinates
x<- ozone2$lon.lat
# y is a vector of ozone measurements at day 16. Note some missing values.
y<- ozone2$y[16,]
system.time( obj<- spatialProcess( x, y))
system.time( obj<- spatialProcess( x, y, theta=2.5))
names( obj)
names( obj$MLEInfo)
obj$summary
obj$MLEInfo$pars.MLE
